strong > strong[foo $= bar] {
    font-style: italic;
}

p + p[foo^=bar]{
    font-family: Helvetica;
}

p +p[foo=bar], p~div[asdf] {
    color: #ff0000;
    #id >span, a:hover {
        color: #123456;
    }
}

[class~=big] {
    font-size: x-large;
}

*[class|=top] {
    border-top: 1px solid;
}

*[class*=hidden] {
    display: none;
}